<Window x:Class="StockTrading.Desktop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:uc="clr-namespace:StockTrading.Desktop.SharedControls" xmlns:uc1="clr-namespace:StockTrading.Desktop.HomeControls"
        mc:Ignorable="d"
        Title="MainWindow" Height="700" Width="1200" Loaded="MainWindow_Loaded">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"></ColumnDefinition>
            <ColumnDefinition Width="8*"/>
        </Grid.ColumnDefinitions>
        <Rectangle Fill="#A8AFAE" Grid.Column="0"></Rectangle>
        <!--Navigation-->
        <uc:NavigationControl Grid.Column="0"></uc:NavigationControl>

        <!--Page Content Display-->
        <Frame x:Name="contentFrame" Content="Frame" Grid.Column="1" Width="Auto" NavigationUIVisibility="Hidden"/>





    </Grid>
</Window>
